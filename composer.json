{
    "name": "ddeboer/vatin-bundle",
    "description": "Symfony bundle for the VATIN library",
    "keywords": [ "vat", "tax", "btw" ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David de Boer",
            "email": "david@ddeboer.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ddeboer/vatin": "^2.0",
        "symfony/http-kernel": "^4.0 || ^5.0 || ^6.0 || ^7.0",
        "symfony/dependency-injection": "^4.0 || ^5.0 || ^6.0 || ^7.0",
        "symfony/config": "^4.0 || ^5.0 || ^6.0 || ^7.0",
        "symfony/validator": "^4.0 || ^5.0 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^8.5 || ^9.0",
        "doctrine/annotations": "^1.2",
        "symfony/framework-bundle": "^5.0 || ^6.0 || ^7.0",
        "symfony/yaml": "^5.0 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ddeboer\\VatinBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ddeboer\\VatinBundle\\Tests\\": "tests"
        }
    }
}
